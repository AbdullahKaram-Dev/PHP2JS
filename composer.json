{
    "name": "rmunate/php2js",
    "description": "Lea las variables de PHP LARAVEL en un archivo externo de JAVASCRIPT sin necesidad de hacer peticiones AJAX, FETCH o AXIOS, use las mismas variables del archivo retornado por el controlador.",
    "keywords": [
        "php2js",
        "__php",
        "altum",
        "rmunate"
    ],
    "homepage": "https://github.com/rmunate/PHP2JS",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rmunate\\Php2Js\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Raul Mauricio UÃ±ate Castro",
            "email": "raulmauriciounate@gmail.com",
            "role": "Developer",
            "homepage": "https://github.com/rmunate"
        },
        {
            "name": "Wilmer Alonso Sanchez Saez",
            "email": "wasanchez@serdan.com.co",
            "role": "Developer",
            "homepage": "https://github.com/wilmersaz"
        },
        {
            "name": "John Alejandro Diaz Pinilla",
            "email": "jdiaz@serdan.com.co",
            "role": "QA",
            "homepage": "https://github.com/alejandrodiazpinilla"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        
    },
    "extra" : {
        "branch-alias" : {
            "dev-main": "v2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Rmunate\\Php2Js\\PHP2JSServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
